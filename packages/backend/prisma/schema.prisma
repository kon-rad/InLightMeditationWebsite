// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id        String   @id @default(uuid())
  name  String
  email String
  image   String
  password   String
}

model Session {
  id        String   @id @default(uuid())
  title  String
  type String
  note   String
  datetime   String
}